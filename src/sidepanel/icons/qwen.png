            dim1: "main"
        });
        window.aes = aes;

        var AES_PLUGINS = aes.use([AESPluginPV,AESPluginResourceError,AESPluginAutolog,AESPluginEventTiming]);
        
        aes.use(AESPluginPerf, {
            plugin_perf_enableUserTimingObserve: true,
            plugin_perf_enableResourceTiming: true,
            plugin_perf_resourceTimingThreshhold: 10000,
            plugin_perf_resourceTimingSampling: 0.1,
        });

        aes.use(AESPluginBlank, {
            plugin_blank_target: '#ice-container',
            plugin_blank_when: ['error', 'leave'],
            plugin_blank_tester: function(target) {
                if (target) {
                    if (target.children.length === 0) return false;
                    return true;
                } else {
                    return false;
                }
            }
        });

        window.aesSendError = aes.use(AESPluginJSError, {
            disable_unhandled_rejection: true,
        }).sendError;
        aesSendError();

        window.aesSendApi = aes.use(AESPluginAPI, {
            sendResponseOnSuccess: false,
        }).sendApi;

        window.aesSendEvent = aes.use(AESPluginEvent);

        window.addEventListener('error', function(e) {
            if (!(e instanceof ErrorEvent)) {
                var target = e.target;
                var src = target.src || target.href;
                if(!src || src.indexOf('.mmstat.com') !== -1 || /aes-\d+\.cn-wulanchabu\.log\.aliyuncs\.com/.test(src)){
                    return;
                }
                var a = document.createElement('a');
                a.href = src;

                try {
                    aesSendEvent('resource_error', {
                        et: 'OTHER',
                        c1: a.host,
                        c2: src,
                    });
                }
                catch(e) {
                    console.error(e);
                }
            }
        }, true)

        window.addEventListener('unhandledrejection', function(e) {
            var info = e.reason;

            if (info instanceof Error) {
                aesSendError(info);
            } else if (info) {
                var traceId = info.traceId;
                var errorCode = info.errorCode;
                var data = info.data;
                
                delete info.traceId;
                delete info.errorCode;
                delete info.data;

                try {
                    info = typeof info === 'string' ? info : JSON.stringify(info);
                } catch (e) {
                    info = 'JSON.stringify error';
                }

                aesSendError({
                    type: 'rejection_error',
                    message: info,
                });

                if (traceId || errorCode || data) {
                    aesSendEvent('rejection_error', {
                        et: 'OTHER',
                        c1: errorCode,
                        c2: traceId,
                        c3: data ? JSON.stringify(data) : '',
                    });
                }
            } else {
                aesSendError({
                    type: 'rejection_error',
                    message: info,
                });
            }
        });
    </script>
