                <div class="skeleton" style="width: 232px; height: 20px; margin-bottom: 12px"></div>
                <div class="skeleton" style="width: 160px; height: 20px; margin-bottom: 12px"></div>
            </div>
            <div style="flex: 1; display: flex; flex-direction: column">
            <div style="flex: 1; padding: 116px 101px 0 133px; background: var(--background-primary)">
                <div class="skeleton" style="width: 100%; height: 20px; margin-bottom: 20px"></div>
                <div class="skeleton" style="width: 38.88%; height: 20px; margin-bottom: 12px"></div>
            </div>
        </div>
    </div>

    <script >
        if (originUA.toLowerCase().includes('ucbrowser')) {
            localStorage.setItem('tongyi-theme-preference', 'auto');
        }

        const mode = localStorage.getItem('tongyi-theme-preference') || 'auto';
        const actualMode = mode === 'auto' ? (window.matchMedia('(prefers-color-scheme: dark)').matches ? 'dark' : 'light') : mode;

        const backgroundPrimary = { light: '#fff', dark: '#151515', }[actualMode];
        const backgroundPCSidebar = { light: '#f8f8f9', dark: '#0f0f0f', }[actualMode];
        const linearGradient = { 
            light: 'linear-gradient(86deg, rgba(17, 17, 51, 0.03) 38%, rgba(17, 17, 51, 0.06) 51%, rgba(17, 17, 51, 0.03) 66%)', 
            dark: 'linear-gradient(90deg, #212121 28%, #2C2C2C 53%, #212121 81%)',
        }[actualMode]

        const skeletonDom = document.querySelector('#skeleton');
        if(skeletonDom) {
            skeletonDom.style.setProperty('--background-pc-sidebar', backgroundPCSidebar);
            skeletonDom.style.setProperty('--linear-gradient', linearGradient);
        }
        const body = document.querySelector('body');
        body.style.setProperty('--background-primary', backgroundPrimary);
        body.style.setProperty('background-color', 'var()');
    </script>

    <script >
        function hideSkeleton() {
            const $skeleton = document.querySelector('#skeleton');
            if($skeleton && $skeleton.remove) {
                $skeleton.remove();
            }
        }
        if (window.MutationObserver) {
            const observer = new MutationObserver(() => {
                const root = document.querySelector('#ice-container');
                if (root && root.children.length > 0) {
                    hideSkeleton();
                    observer.disconnect();
                }
            });

            observer.observe(document.querySelector('#ice-container'), { childList: true });
        } else {
            window.addEventListener('DOMContentLoaded', function() {
                hideSkeleton();
            })
        }
    </script>
